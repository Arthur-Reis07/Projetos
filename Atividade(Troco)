//Início
Double valor1, valor2, valor3, valor4, valor5, valorpagamento, troco;
Console.WriteLine("Digite o valor do pagamento:");
valorpagamento=Convert.ToDouble(Console.ReadLine());
Console.WriteLine("Digite o primeiro valor:");
valor1 = Convert.ToDouble(Console.ReadLine());
Console.WriteLine("Digite o segundo valor:");
valor2 = Convert.ToDouble(Console.ReadLine());
Console.WriteLine("Digite o terceiro valor:");
valor3 = Convert.ToDouble(Console.ReadLine());
Console.WriteLine("Digite o quarto valor:");
valor4 = Convert.ToDouble(Console.ReadLine());
Console.WriteLine("Digite o quinto valor:");
valor5 = Convert.ToDouble(Console.ReadLine());
troco=valorpagamento-(valor1+valor2+valor3+valor4+valor5);
Console.WriteLine("O seu troco é: " + troco);
//Fim
